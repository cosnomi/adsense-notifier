{
  "name": "adsense-notifier",
  "version": "0.0.1",
  "main": "dest/index.js",
  "repository": "git@github.com:cosnomi/adsense-notifier.git",
  "author": "Cosnomi <cosnomi@cosnomi.com>",
  "license": "MIT",
  "scripts": {
    "build": "npx tsc",
    "prebuild": "sed -i -e 's/version: '\\''v1_4'\\''/version: '\\''v1.4'\\''/g' ./node_modules/googleapis/build/src/apis/adsense/v1.4.d.ts",
    "test": "npx jest",
    "start": "npx tsc && node dest/index.js"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jest": "^24.0.12",
    "@types/node": "^11.13.0",
    "@types/readline-sync": "^1.4.3",
    "jest": "^24.7.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "googleapis": "^39.2.0",
    "readline-sync": "^1.4.9",
    "zaim": "^2.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ]
  }
}
